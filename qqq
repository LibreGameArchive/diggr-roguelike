[ 'Australopithecus afarensis' ('peasant' monsterstock_get {
    skin=Skin{ char='h' color='sepia' } name='Australopithecus afarensis' branch='a' defence=0.3 range=4 
    no_a=true desc='An early hominid, this creature walked upright\nbut lacked the intelligence of the modern human.' 
    } ) ]

